
struct B
{
	int w;
	int add[4];
	int dis[4];
}

struct A
{
	int cnt;
	struct B b[4];
};

struct A p[7];


void init()
{
	//0
	p[0].cnt = 2;
	//00
	p[0].b[0].w = 4;
	p[0].b[0].add[0] = 1;
	p[0].b[0].add[1] = 1;
	p[0].b[0].add[2] = 1;
	p[0].b[0].add[3] = 1;
	p[0].b[0].dis[0] = 0;
	p[0].b[0].dis[0] = 0;
	p[0].b[0].dis[1] = 0;
	p[0].b[0].dis[2] = 0;
	p[0].b[0].dis[3] = 0;
	//01
	p[0].b[1].w = 1;
	p[0].b[1].add[0] = 4;
	p[0].b[1].dis[0] = 0;


	//1
	p[1].cnt = 4;
	//10
	p[1].b[0].w = 3;
	p[1].b[0].add[0] = 2;
	p[1].b[0].add[1] = 1;
	p[1].b[0].add[2] = 1;
	p[1].b[0].dis[0] = 0;
	p[1].b[0].dis[1] = 0;
	p[1].b[0].dis[2] = 0;
	//11

}
